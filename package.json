{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/history": "^4.7.9",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.20",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "formik": "^2.2.9",
    "history": "^5.1.0",
    "node-sass": "^7.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          [
            "constants",
            "./src/constants"
          ],
          [
            "config",
            "./src/config"
          ],
          [
            "components",
            "./src/components"
          ],
          [
            "api",
            "./src/api"
          ],
          [
            "utils",
            "./src/utils"
          ],
          [
            "actions",
            "./src/actions"
          ],
          [
            "reducers",
            "./src/reducers"
          ],
          [
            "schemas",
            "./src/schemas"
          ],
          [
            "store",
            "./src/store"
          ],
          [
            "storage",
            "./src/storage"
          ],
          [
            "hooks",
            "./src/hooks"
          ],
          [
            "tests",
            "./src/tests"
          ],
          [
            "styles",
            "./src/styles"
          ],
          [
            "fixtures",
            "./src/fixtures"
          ]
        ]
      }
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^(components|config|constants|actions|reducers|styles|api|hooks|utils|store|storage|schemas|tests)$": "<rootDir>/src/$1"
    }
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ],
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
